{
  "name": "liyao-vue-common",
  "version": "1.0.6",
  "private": false,
  "type": "module",
  "description": "A upload component for Vue3 and element-plus",
  "keywords": [
    "Vue3",
    "element-plus",
    "component",
    "upload"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "types/index.d.ts",
  "style": "dist/style.css",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liyao52033/liyao-vue-common.git"
  },
  "bugs": {
    "url": "https://github.com/liyao52033/liyao-vue-common/issues"
  },
  "homepage": "https://github.com/liyao52033/liyao-vue-common",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "pub": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^12.8.2",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.5",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.52.2",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^8.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.9",
    "@types/path-browserify": "^1.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "@wangeditor/editor": "^5.1.23",
    "cz-git": "^1.11.1",
    "sass-embedded": "^1.86.0",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  },
  "peerDependencies": {
    "element-plus": "^2.9.6",
    "vite": "^6.2.0"
  },
  "license": "MIT"
}